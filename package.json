    },
    "keywords": [
        "prettier",
        "prettier-config"
    ],
    "author": {
        "name": "Nicolas Hedger",
        "email": "nicolas@hedger.ch"
    },
    "devDependencies": {
        "@types/node": "^18.7.3",
        "@types/prettier": "^2.7.0",
        "prettier": "^2.7.1",
        "semantic-release": "^19.0.3",
        "unbuild": "^0.8.8"
    }
}
