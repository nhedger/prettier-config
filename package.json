{
    "name": "@hedger/prettier-config",
    "version": "1.0.0",
    "description": "My personal Prettier configuration",
    "repository": {
        "type": "git",
        "url": "git@github.com:nhedger/prettier-config.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
